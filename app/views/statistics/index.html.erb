<h4>Import Internet and Mobile Data</h4>
<%= form_tag import_internet_mobile_users_statistics_path, multipart: true do %>
  <span class="btn btn-file" >
    Upload File
    <%= file_field_tag :file %>
  </span>
  <%= submit_tag "Import", class: 'btn blue' %>
<% end %>

<h3>Internet and Mobile User Trends</h3>
<div class="row">
  <div class="col s6">
    <span id="mobile">
      <%= mobile_users %>
    </span>
    <input type="button" id="svMobile" value="Save PNG"/>
    <%= link_to 'Download', import_internet_mobile_users_statistics_path("csv"), class: "btn blue" %>
  </div>
  <div class="col s6">
    <span id="internet">
      <%= internet_users %>
    </span>
    <input type="button" id="svInternet" value="Save PNG"/>
    <%= link_to 'Download', import_internet_mobile_users_statistics_path("csv"), class: "btn blue" %>
  </div>
</div>

