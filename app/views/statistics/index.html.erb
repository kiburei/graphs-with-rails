<h4>Import Internet and Mobile Data</h4>
<%= form_tag import_internet_mobile_users_statistics_path, multipart: true do %>
  <span class="btn btn-file" >
    Upload File
    <%= file_field_tag :file %>
  </span>
  <%= submit_tag "Import", class: 'btn blue' %>
<% end %>

<h3>Internet and Mobile User Trends</h3>
<div class="row">
  <div class="col s6">
    <%= mobile_users %>
  </div>
  <div class="col s6">
    <%= internet_users %>
  </div>
</div>
